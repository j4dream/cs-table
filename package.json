{
  "name": "virtualized-sc-table",
  "version": "0.5.1",
  "private": false,
  "main": "./dist/main",
  "files": [
    "dist"
  ],
  "homepage": "https://j4dream.github.io/cs-table/",
  "license": "MIT",
  "keywords": [
    "react",
    "table",
    "virtualized",
    "resizable",
    "resize"
  ],
  "dependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "commit": "git-cz",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "prettier": "prettier -c --write \"**/*\"",
    "publish:npm": "babel ./src -d dist --extensions \".ts,.js,.tsx\" --copy-files --no-copy-ignored",
    "predeploy": "npm run docz:build",
    "deploy": "gh-pages -d .docz/dist"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.9",
    "cz-conventional-changelog": "^3.2.0",
    "docz": "^2.3.1",
    "gh-pages": "^2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.differenceby": "^4.8.0",
    "lodash.remove": "^4.7.0",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1",
    "typescript": "^3.9.3"
  }
}
